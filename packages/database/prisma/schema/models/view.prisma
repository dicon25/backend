model PaperView {
  id String @id @default(uuid())

  userId String?
  user   User?   @relation(fields: [userId], references: [id], onDelete: Cascade)

  paperId String
  paper   Paper  @relation(fields: [paperId], references: [id], onDelete: Cascade)

  createdAt DateTime @default(now())

  @@index([userId])
  @@index([paperId])
  @@index([createdAt])
}
